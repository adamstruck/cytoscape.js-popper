{
  "name": "cytoscape-popper",
  "version": "0.0.1",
  "description": "A Cytoscape.js extension for Popper.js",
  "main": "cytoscape-popper.js",
  "author": "Cytoscape",
  "scripts": {
    "gh-pages-deploy": "npm run build && gh-pages -d .",
    "lint": "eslint src",
    "build": "cross-env NODE_ENV=production webpack",
    "build:min": "cross-env NODE_ENV=production MIN=true webpack",
    "watch": "webpack --progress --watch",
    "dev": "webpack-dev-server --open",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cytoscape/cytoscape.js-popper.git"
  },
  "keywords": [
    "cytoscape",
    "cytoscape-extension"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cytoscape/cytoscape.js-popper/issues"
  },
  "homepage": "https://github.com/cytoscape/cytoscape.js-popper",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "camelcase": "^4.1.0",
    "chai": "4.0.2",
    "cross-env": "^5.0.0",
    "eslint": "^3.9.1",
    "gh-pages": "^1.0.0",
    "mocha": "3.4.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "cytoscape": "^3.2.0"
  },
  "dependencies": {
    "popper.js": "^1.0.0"
  }
}
